{
    "name": "scoundrel-card-game",
    "version": "0.0.0",
    "description": "Scoundrel card game(designed by Zach Gage and Kurt Bieg) built with PixiJS and TypeScript",
    "browserslist": "> 0.25%, not dead",
    "engines": {
        "node": "16.18.0"
    },
    "scripts": {
        "start": "npm run dev",
        "dev": "webpack-dev-server -- --env mode=development --hot",
        "build": "webpack -- --env mode=production",
        "test": "npx jest",
        "prebuild": "npm run clean",
        "clean": "npx rimraf dist",
        "code-coverage": "jest --coverage",
        "prettier": "npx prettier",
        "prettier:format": "npm run prettier -- --write src/**/*.ts",
        "prettier:check": "npm run prettier -- -c ./src/**/*.ts",
        "lint-check": "npx eslint ./src/**/*.ts",
        "code-style-check": "npm run lint-check && npm run prettier:check",
        "prepare": "husky install"
    },
    "dependencies": {
        "gl-matrix": "^3.4.4",
        "mobx": "^6.15.0",
        "motion": "^12.23.24",
        "pixi.js": "^8.14.0",
        "redux-saga": "^1.4.2",
        "seedrandom": "^3.0.5"
    },
    "devDependencies": {
        "@swc/core": "^1.3.11",
        "@types/jest": "^29.2.0",
        "@types/node": "^18.11.7",
        "@types/seedrandom": "^3.0.8",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.41.0",
        "browserslist": "^4.21.4",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-webpack-plugin": "^3.2.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.2.2",
        "jest-canvas-mock": "^2.4.0",
        "jsdom": "^20.0.1",
        "mini-css-extract-plugin": "^2.6.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "regenerator-runtime": "^0.13.10",
        "rimraf": "^3.0.2",
        "swc-loader": "^0.2.3",
        "terser-webpack-plugin": "^5.3.6",
        "tiled-types": "^1.3.0",
        "tmx-map-parser": "^1.3.1",
        "ts-jest": "^29.0.3",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.9.3",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1",
        "webpack-free-tex-packer": "^0.3.4",
        "webpack-manifest-plugin": "^5.0.0",
        "webpack-merge": "^5.8.0"
    },
    "author": "Mykyta Dniprovskyi"
}
